mv -v *~       /tmp 2> /dev/null
mv -v *.tar.gz /tmp 2> /dev/null
mv -v *.zip    /tmp 2> /dev/null
mv -v *.png    /tmp 2> /dev/null
mv -v *.JPG    /tmp 2> /dev/null
mv -v *.JPEG   /tmp 2> /dev/null
mv -v *.jpg    /tmp 2> /dev/null
mv -v *.jpeg   /tmp 2> /dev/null
mv -v *.pdf    /tmp 2> /dev/null
mv -v *.docx   /tmp 2> /dev/null
mv -v \#*      /tmp 2> /dev/null
mv -v .*~*     /tmp 2> /dev/null
